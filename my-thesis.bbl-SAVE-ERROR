      \strng{namehash}{776e660f143301a84cddbaf1416fd17c}
      \strng{fullhash}{f415bd3185350980192fd50799dc35f5}
      \strng{bibnamehash}{94b21cab2445071aaf69bfe131f026f7}
      \strng{authorbibnamehash}{94b21cab2445071aaf69bfe131f026f7}
