    \entry{Stoica1988}{article}{}
      \name{author}{2}{}{%
        {{hash=36fee71d3ae6d96d0ed40d1edb70559a}{%
           family={Stoica},
           familyi={S\bibinitperiod},
           given={Petre},
           giveni={P\bibinitperiod}}}%
        {{hash=626bcd16f0f7e6a0d0eee823f81e4c01}{%
