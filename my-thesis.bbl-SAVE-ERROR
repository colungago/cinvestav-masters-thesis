      \strng{namehash}{e6864cb0ca5946f24a083c75b12240e6}
      \strng{fullhash}{e6864cb0ca5946f24a083c75b12240e6}
      \strng{bibnamehash}{e6864cb0ca5946f24a083c75b12240e6}
      \strng{authorbibnamehash}{e6864cb0ca5946f24a083c75b12240e6}
