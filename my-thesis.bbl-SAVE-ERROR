      \strng{namehash}{b45165aa909f2ccbbad222f07bea16f7}
      \strng{fullhash}{0d683f999d8be6e6e86e795a19eac506}
      \strng{bibnamehash}{0d683f999d8be6e6e86e795a19eac506}
      \strng{authorbibnamehash}{0d683f999d8be6e6e86e795a19eac506}
      \strng{authornamehash}{b45165aa909f2ccbbad222f07bea16f7}
