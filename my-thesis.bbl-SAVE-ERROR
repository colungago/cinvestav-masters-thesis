      \strng{namehash}{02f1193e118b2f60fe049e60172876ee}
      \strng{fullhash}{02f1193e118b2f60fe049e60172876ee}
      \strng{bibnamehash}{02f1193e118b2f60fe049e60172876ee}
      \strng{authorbibnamehash}{02f1193e118b2f60fe049e60172876ee}
      \strng{authornamehash}{02f1193e118b2f60fe049e60172876ee}
