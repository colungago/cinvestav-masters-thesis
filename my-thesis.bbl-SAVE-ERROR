      \strng{namehash}{e1a8b265e79847c2fac1b9870c3c0bfb}
      \strng{fullhash}{5eed6a19aa363208bc89fcdd7c5532e2}
      \strng{bibnamehash}{803170c50912d70ae2c71964cdfb4d8b}
      \strng{authorbibnamehash}{803170c50912d70ae2c71964cdfb4d8b}
      \strng{authornamehash}{e1a8b265e79847c2fac1b9870c3c0bfb}
