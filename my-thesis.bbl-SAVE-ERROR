      \strng{namehash}{86b1ce130e711cf0d4230b35087e2d67}
      \strng{fullhash}{c0ca0595296c64ddc6e87a08c4dabd09}
      \strng{bibnamehash}{c0ca0595296c64ddc6e87a08c4dabd09}
      \strng{authorbibnamehash}{c0ca0595296c64ddc6e87a08c4dabd09}
