    \entry{Cohen1983}{article}{}
      \name{author}{2}{}{%
        {{hash=4c413160abaa666575a0b3ecf9260ef6}{%
           family={Cohen},
           familyi={C\bibinitperiod},
           given={David},
           giveni={D\bibinitperiod}}}%
        {{hash=8458b8f93712aaecde6807258e193661}{%
           family={Cuffin},
           familyi={C\bibinitperiod},
           given={B.\bibnamedelimi Neil},
           giveni={B\bibinitperiod\bibinitdelim N\bibinitperiod}}}%
