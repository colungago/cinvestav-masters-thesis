    \entry{open2}{article}{}
      \name{author}{6}{}{%
        {{hash=30b2acf1812142b2fef6638347a3c294}{%
           family={Kybic},
           familyi={K\bibinitperiod},
           given={Jan},
           giveni={J\bibinitperiod}}}%
        {{hash=6f2c567b1e82716f593cd6972db2e588}{%
           family={Clerc},
