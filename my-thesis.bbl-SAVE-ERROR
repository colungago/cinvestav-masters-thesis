      \field{journaltitle}{NeuroImage}
      \field{number}{1}
      \field{title}{A New Improved Version of the Realistic Digital Brain Phantom}
      \field{volume}{32}
      \field{year}{2006}
      \field{pages}{138\bibrangedash 145}
      \range{pages}{8}
      \verb{doi}
      \verb 10.1016/j.neuroimage.2006.03.052
