      \strng{namehash}{a0eae38de72ce468525b60995984c791}
      \strng{fullhash}{b96aaf07eed6504afccee4ae65a5bdc4}
      \strng{bibnamehash}{4f250798a6400273a53139297ba11c8b}
      \strng{authorbibnamehash}{4f250798a6400273a53139297ba11c8b}
