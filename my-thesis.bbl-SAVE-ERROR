    \entry{jimenez-cruzReducedrankBeamformingBrain2019}{inproceedings}{}
      \name{author}{2}{}{%
        {{hash=7ec2c59a729965ea5884a0d215a6de75}{%
           family={{Jimenez-Cruz}},
