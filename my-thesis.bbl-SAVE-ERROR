    \entry{McCann2019}{article}{}
      \name{author}{3}{}{%
        {{hash=1547724c206604ed907b00c43bdec90e}{%
           family={McCann},
           familyi={M\bibinitperiod},
           given={Hannah},
           giveni={H\bibinitperiod}}}%
        {{hash=f1aab6b40995bc6497b115c59cb098f7}{%
           family={Pisano},
           familyi={P\bibinitperiod},
           given={Giampaolo},
           giveni={G\bibinitperiod}}}%
        {{hash=56cdfff737f0651cea3e44f9e43efeb3}{%
